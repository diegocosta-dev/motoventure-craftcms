<!DOCTYPE html>

<html lang="en-US">
  {% minify %}
    <head>
        {% hook "seo" %}

        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width,minimum-scale=1" />
        <meta name="referrer" content="origin-when-cross-origin" />
        <link rel="shortcut icon" href="{{ siteUrl }}static/favicon.ico" type="image/x-icon" />
        <link rel="preconnect" href="https://fonts.gstatic.com">
        <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,400;0,500;0,700;0,900;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
        <link rel="stylesheet" href="{{ siteUrl }}static/css/styles.css">
        
    </head>
    {% block bodyTag %}
      <body>
    {% endblock %}

      <div class="wrapper">

        {{ include('_partials/_siteHeader') }}

        {% block content %}{% endblock %}

      </div>

      {{ include('_partials/_siteFooter') }}

      <script src="{{ siteUrl }}static/js/scripts.js"></script>

    </body>
  {% endminify %}
</html>